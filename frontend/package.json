    "element-ui": "^2.15.14",
    "vue": "^2.7.15",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.6.5",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
